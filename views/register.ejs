<body id="page-top">
  <div class="container-fluid text-center log-bg">
    <div class="wrapper">
      <h1 class="mb-4">Regisztráció</h1>
      <form action="/register" method="POST">
        <div class="form-row">
          <div class="form-group col-md-8 mx-auto">
            <input class="form-control" type="email" name="email" placeholder="email cím" autofocus>
          </div>
          <div class="form-group col-md-8 mx-auto">
            <input class="form-control" type="text" name="username" placeholder="felhasználónév">
          </div>
          <div class="form-group col-md-8 mx-auto">
            <input class="form-control" type="password" name="password" placeholder="jelszó">
          </div>

          <div class="form-group col-md-8 mx-auto">
            <input class="form-control" type="password" name="samePassword" placeholder="jelszó mégegyszer">
          </div>
        </div>

        <button class="btn btn-md btn-primary">Regisztrálok</button>
        <a class="btn btn-md btn-dark" href="/">Vissza</a>
      </form>
      </div>

      </div>

<script src="https://code.jquery.com/jquery-3.3.1.slim.min.js"
